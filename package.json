{
  "name": "marionette-browserify-gulp-express-starter",
  "version": "0.0.1",
  "description": "Marionette starter with express API using browserify and gulp",
  "browser": {
    "jquery": "./bower_components/jquery/jquery.js",
    "underscore": "./bower_components/underscore/underscore.js",
    "backbone": "./bower_components/backbone/backbone.js",
    "backbone.wreqr": "./bower_components/backbone.wreqr/lib/backbone.wreqr.js",
    "backbone.babysitter": "./bower_components/backbone.babysitter/lib/backbone.babysitter.js",
    "backbone.marionette": "./bower_components/backbone.marionette/lib/backbone.marionette.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": "./config/shim.js",
  "devDependencies": {
    "bower": "~1.3.7",
    "browser-sync": "~0.8.2",
    "browserify-shim": "~3.4.1",
    "browserify": "~3.36.0",
    "coffeeify": "~0.6.0",
    "connect": "~2.14.3",
    "gulp-changed": "~0.3.0",
    "gulp-compass": "~1.1.8",
    "gulp-imagemin": "~0.1.5",
    "gulp-notify": "~1.2.4",
    "gulp-open": "~0.2.8",
    "gulp": "~3.6.0",
    "hbsfy": "~1.3.2",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~0.10.1",
    "gulp-util": "~2.2.14",
    "pretty-hrtime": "~0.2.1"
  },
  "dependencies": {
    "express": "~4.5.1",
    "mongoose": "~3.8.3",
    "MD5": "~1.2.0",
    "errorhandler": "^1.0.2",
    "morgan": "^1.1.1",
    "method-override": "^2.0.2",
    "body-parser": "^1.3.0"
  }
}
