{
  "name": "marionette-browserify-gulp-express-starter",
  "version": "0.0.1",
  "description": "Marionette starter with express API using browserify and gulp",
  "repository": {
    "type": "git",
    "url": "https://github.com/dqdinh/marionette-browserify-gulp-express-starter.git"
  },
  "browser": {
    "bootstrap-sass": "~3.0.2",
    "jqueryui": "./client/javascript/vendor/jquery-ui-browserify-shimmed.js",
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "jquery.spin": "./client/javascript/vendor/jquery-spin-browserify-shimmed.js",
    "spin": "./bower_components/spin.js/spin.js",
    "lodash": "./bower_components/lodash/dist/lodash.underscore.js",
    "backbone": "./bower_components/backbone/backbone.js",
    "backbone.wreqr": "./bower_components/backbone.wreqr/lib/backbone.wreqr.js",
    "backbone.babysitter": "./bower_components/backbone.babysitter/lib/backbone.babysitter.js",
    "backbone.marionette": "./bower_components/backbone.marionette/lib/backbone.marionette.js",
    "backbone.picky": "./bower_components/backbone.picky/lib/backbone.picky.js",
    "backbone.syphon": "./bower_components/backbone.syphon/lib/backbone.syphon.js",
    "backbone.localstorage": "./bower_components/backbone.localstorage/backbone.localStorage.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": "./config/shim.js",
  "devDependencies": {
    "bower": "~1.3.8",
    "browser-sync": "~1.2.1",
    "browserify-shim": "~3.6.0",
    "browserify": "~4.2.0",
    "coffeeify": "~0.6.0",
    "connect": "~3.0.2",
    "gulp-changed": "~0.4.0",
    "gulp-compass": "~1.1.9",
    "gulp-imagemin": "~0.6.1",
    "gulp-notify": "~1.4.0",
    "gulp-open": "~0.2.8",
    "gulp-nodemon": "~1.0.4",
    "gulp-shell": "~0.2.8",
    "gulp-util": "~2.2.14",
    "gulp": "~3.8.5",
    "hbsfy": "~1.3.2",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~0.10.1",
    "karma": "~0.12.17",
    "karma-coverage": "~0.2.4",
    "karma-phantomjs-launcher": "~0.1.4",
    "karma-mocha-reporter": "~0.2.8",
    "karma-chai": "~0.1.0",
    "karma-mocha": "~0.1.4",
    "lodash": "~2.4.1",
    "pretty-hrtime": "~0.2.1"
  },
  "dependencies": {
    "express": "~4.5.1",
    "mongoose": "~3.8.3",
    "MD5": "~1.2.0",
    "errorhandler": "^1.0.2",
    "morgan": "^1.1.1",
    "method-override": "^2.0.2",
    "body-parser": "^1.3.0"
  }
}
